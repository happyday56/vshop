<?xml version="1.0" encoding="utf-8"?>
<!--核心设置-->
<Setting>
  <!--全局调试-->
  <Debug>false</Debug>
  <!--日志等级，只输出大于等于该级别的日志-->
  <LogLevel>Info</LogLevel>
  <!--文本日志目录-->
  <LogPath>../Log</LogPath>
  <!--临时目录-->
  <TempPath>XTemp</TempPath>
  <!--扩展插件存放目录-->
  <PluginPath>Bin</PluginPath>
  <!--网络调试。默认位于系统盘的X\Cache-->
  <DownloadCache>C:\X\Cache</DownloadCache>
  <!--网络调试-->
  <NetDebug>false</NetDebug>
  <!--多线程调试-->
  <ThreadDebug>false</ThreadDebug>
  <!--网页压缩文件-->
  <WebCompressFiles>.aspx,.axd,.js,.css</WebCompressFiles>
</Setting>